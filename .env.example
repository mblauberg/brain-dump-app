# AI Provider Configuration
# Uncomment and fill in the API key for your preferred provider

# OpenAI Configuration
# REACT_APP_OPENAI_API_KEY=your-openai-api-key-here
# REACT_APP_OPENAI_MODEL=gpt-4-turbo-preview  # Options: gpt-4-turbo-preview, gpt-4, gpt-3.5-turbo

# Anthropic Claude Configuration
# REACT_APP_CLAUDE_API_KEY=your-claude-api-key-here
# REACT_APP_CLAUDE_MODEL=claude-3-opus-20240229  # Options: claude-3-opus-20240229, claude-3-sonnet-20240229, claude-3-haiku-20240307

# Google Gemini Configuration
# REACT_APP_GEMINI_API_KEY=your-gemini-api-key-here
# REACT_APP_GEMINI_MODEL=gemini-pro  # Options: gemini-pro, gemini-pro-vision

# Default AI Provider (openai, claude, or gemini)
REACT_APP_DEFAULT_AI_PROVIDER=openai

# Optional: AI Request Configuration
REACT_APP_AI_MAX_TOKENS=2000
REACT_APP_AI_TEMPERATURE=0.7
REACT_APP_AI_REQUEST_TIMEOUT=30000  # 30 seconds

# Optional: Enable AI response caching (reduces API calls)
REACT_APP_ENABLE_AI_CACHE=true
REACT_APP_AI_CACHE_DURATION=3600000  # 1 hour in milliseconds